{
  "name": "apply",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --hot --open --env isBuild=false --env isLocal=true",
    "build": "webpack --config webpack.config.js  --env isBuild=true",
    "ci:build:dev": "webpack --config webpack.config.js --env isBuild=true",
    "ci:build:local": "webpack --config webpack.config.js  --env isBuild=true --env isLocal=true",
    "ci:build:prod": "webpack --config webpack.config.js  --env isBuild=true --env isProd=true --env isLocal=false",
    "ci:build:int": "webpack --config webpack.config.js --env isBuild=true --env isInteg=true",
    "dev:codecoverage": "jest --collect-coverage",
    "prepare": "cd .. && husky install apply/.husky",
    "test": "jest --no-cache -w 2",
    "test:pipeline": "jest --no-cache -w 2 --coverage --silent",
    "dev:test": "jest --watch",
    "lint": "eslint src/**/*.ts src/**/*.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
