{
  "name": "@gcs-ui-controls/react",
  "version": "0.4.70",
  "author": "Microsoft Corporation",
  "description": "Library of reusable React components in GCS",
  "license": "UNLICENSED",
  "files": [
    "lib*"
  ],
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib-commonjs",
    "test": "jest --no-cache -w 2",
    "test:pipeline": "jest --no-cache -w 2 --silent --coverage",
    "dev:test": "jest --watch",
    "eject": "react-scripts eject",
    "storybook": "env-cmd -f env/.env.local yarn start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src/**/*.ts src/**/*.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fluentui/react": "^8.112.8",
    "@microsoftit/telemetry-extensions-npm": "^3.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.20.6",
    "i18next": "^21.6.16",
    "jest-junit-reporter": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.16.6",
    "react-scripts": "5.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.3",
    "webpack": "^5.66.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@coherence-design-system/controls": "4.3.0",
    "@fluentui/react-hooks": "^8.6.14",
    "@gcs-ui-controls/translations": "^0.1.58",
    "@m365-admin/collapsible": "^4.0.20",
    "moment": "^2.29.3",
    "pdfjs-dist": "^2.15.349",
    "react-router-dom": "^5.2.0"
  },
  "resolutions": {
    "@babel/traverse": "^7.23.3",
    "tough-cookie": "^4.1.3",
    "word-wrap": "^1.2.5",
    "semver": "^6.3.1",
    "@adobe/css-tools": "^4.3.2"
  }
}
